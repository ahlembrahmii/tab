ALGORITHM Tab
VAR
    s,element1,element2:integr
    t= tab : ARRAY_OF integr[];
     t1= tab : ARRAY_OF integr[];
BEGIN
    s:=0;
for(i:=0;i>=t.length-1;i++)
for (j:=0;j>=t1.length-1;j++)
if ([t[i]<>t[j]) then 
element1:=t[i];
end;
end;

for(j:=0;i>=t1.length-1;j++)
for (i:=0;j>=t.length-1;i++)
else if ([t[j]<>t[i]) then 
element2:=t1[j];
s:=element1+element2;
i++;
j++:
end;
end;

END


